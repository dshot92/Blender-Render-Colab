      },
      "source": [
        "#@title ## Download && Install Blender on VM\r\n",
        "import os\r\n",
        "\r\n",
        "blender_version = \"blender2.92.0\" #@param [\"blender2.92.0\",\"blender2.91.2\", \"blender2.83.9\"]\r\n",
        "\r\n",
        "if blender_version == \"blender2.92.0\":\r\n",
        "    download_path=\"https://download.blender.org/release/Blender2.92/blender-2.92.0-linux64.tar.xz\"\r\n",
        "\r\n",
        "if blender_version == \"blender2.91.2\":\r\n",
        "    download_path=\"https://download.blender.org/release/Blender2.91/blender-2.91.2-linux64.tar.xz\"\r\n",
        "\r\n",
        "if blender_version == \"blender2.83.9\":\r\n",
        "    download_path=\"https://download.blender.org/release/Blender2.83/blender-2.83.9-linux64.tar.xz\"\r\n",
        "\r\n",
        "!mkdir $blender_version\r\n",
        "if blender_version == \"blender2.92.0\":\r\n",
        "    !wget -O '{blender_version}.tar.xz' -nc $download_path\r\n",
        "    !tar xf '{blender_version}.tar.xz' -C ./$blender_version --strip-components=1\r\n",
        "\r\n",
        "#  This Block is required as some weird behaviors with libtcmalloc appeared in the colab VM\r\n",
        "os.environ[\"LD_PRELOAD\"] = \"\"\r\n",
        "\r\n",
        "!apt update\r\n",
        "!apt remove libtcmalloc-minimal4\r\n",
        "!apt install libtcmalloc-minimal4\r\n",
        "os.environ[\"LD_PRELOAD\"] = \"/usr/lib/x86_64-linux-gnu/libtcmalloc_minimal.so.4.3.0\"\r\n",
        "\r\n",
        "!echo $LD_PRELOAD\r\n",
        "\r\n",
        "# Unpack and move blender\r\n",
        "!apt install libboost-all-dev\r\n",
        "!apt install libgl1-mesa-dev\r\n",
        "!apt install libglu1-mesa libsm-dev"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "QRzNmiHN8Xr0",
        "cellView": "form"
      },
      "source": [
        "#@title Download .blend from Google Drive Share Link\n",
        "import os\n",
        "\n",
        "!mkdir Blend_Files\n",
        "\n",
        "if \"gdrive.sh\" not in os.listdir(\".\"):\n",
        "  !wget https://raw.githubusercontent.com/GitHub30/gdrive.sh/master/gdrive.sh\n",
        "\n",
        "GDrive_Public_Link = \"\" #@param {type : \"string\"}\n",
        "#file_name = \"\" #@param {type : \"string\"}\n",
        "#file_name += \".blend\"\n",
        "\n",
        "url = GDrive_Public_Link.split(\"file/d/\")[1]\n",
        "id = url.split(\"/view?\")[0]\n",
        "\n",
        "file_name = ""\n",
        "if file_name not in os.listdir(\".\"):\n",
        "  !curl gdrive.sh | bash -s $id\n",
        "\n",
        "file_name = \"\"\n",
        "\n",
        "for l in os.listdir():\n",
        "  #print(l[-6:-1])\n",
        "  if l[-6:-1] == \".blen\":\n",
        "    file_name = l\n",
        "    !mv $l ./Blend_Files/$l\n",
        "    !rm $l\n",
        "\n",
        "print(file_name)"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "8dFNjYGTgNjR",
        "cellView": "form"
      },
      "source": [
        "#@title Setup -> Render { run: \"auto\", vertical-output: true, form-width: \"50%\" }\n",
        "#@markdown Scene setup:\n",
        "Samples_Count = 128 #@param {type:\"integer\"}\n",
        "resolution_x = 1920 #@param {type:\"integer\"}\n",
        "resolution_y = 1080 #@param {type:\"integer\"}\n",
        "scale = 1 #@param {type:\"raw\"}\n",
        "\n",
        "#gpu_enabled = True #@param {type:\"boolean\"}\n",
        "gpu_enabled = True \n",
        "#cpu_enabled = False #@param {type:\"boolean\"}\n",
        "cpu_enabled = False\n",
        "\n",
        "#file_name = '60_F40'  #@param {type: \"string\"}\n",
        "#file_name = file_name.split(\".\")[0]\n",
        "#@markdown ---\n",
        "#@markdown Single Frame render\n",
        "single_frame = True #@param {type:\"boolean\"}\n",
        "frame =  1#@param {type:\"integer\"}\n",
        "#@markdown ---\n",
        "#@markdown Sequence render\n",
        "sequence =  False #@param {type:\"boolean\"}\n",
        "start_frame =   1#@param {type:\"integer\"}\n",
        "end_frame =   250#@param {type:\"integer\"}\n",
        "\n",
        "data = \"import re\\n\"+\\\n",
        "    \"import bpy\\n\"+\\\n",
        "    \"scene = bpy.context.scene\\n\"+\\\n",
        "    \"scene.cycles.device = 'GPU'\\n\"+\\\n",
        "    \"bpy.context.scene.cycles.samples = \"+str(Samples_Count)+\"\\n\"+\\\n",
        "    \"bpy.context.scene.render.resolution_x = \"+str(resolution_x * scale)+\"\\n\"+\\\n",
        "    \"bpy.context.scene.render.resolution_y = \"+str(resolution_y * scale)+\"\\n\"+\\\n",
        "    \"prefs = bpy.context.preferences\\n\"+\\\n",
        "    \"prefs.addons['cycles'].preferences.get_devices()\\n\"+\\\n",
        "    \"cprefs = prefs.addons['cycles'].preferences\\n\"+\\\n",
        "    \"print(cprefs)\\n\"+\\\n",
        "    \"# Attempt to set GPU device types if available\\n\"+\\\n",
        "    \"for compute_device_type in ('CUDA', 'OPENCL', 'NONE'):\\n\"+\\\n",
        "    \"    try:\\n\"+\\\n",
        "    \"        cprefs.compute_device_type = compute_device_type\\n\"+\\\n",
        "    \"        print('Device found',compute_device_type)\\n\"+\\\n",
        "    \"        break\\n\"+\\\n",
        "    \"    except TypeError:\\n\"+\\\n",
        "    \"        pass\\n\"+\\\n",
        "    \"for scene in bpy.data.scenes:\\n\"+\\\n",
        "    \"    scene.render.tile_x = 256\\n\"+\\\n",
        "    \"    scene.render.tile_y = 256\\n\"+\\\n",
        "    \"# Enable all CPU and GPU devices\\n\"+\\\n",
        "    \"for device in cprefs.devices:\\n\"+\\\n",
        "    \"    if not re.match('intel', device.name, re.I):\\n\"+\\\n",
        "    \"        print('Activating',device)\\n\"+\\\n",
        "    \"        device.use = \"+str(gpu_enabled)+\"\\n\"+\\\n",
        "    \"    else:\\n\"+\\\n",
        "    \"        device.use = \"+str(cpu_enabled)+\"\\n\"\n",
        "with open('setgpu.py', 'w') as f:\n",
        "    f.write(data)\n",
        "\n",
        "\n",
        "path_to_blend = './Blend_Files/' + file_name\n",
        "output_path = './output_' + file_name + '/output####.png'\n",
        "\n",
        "\n",
        "if sequence:\n",
        "  !sudo ./$blender_version/blender -P './setgpu.py'  -b '{path_to_blend}' -E CYCLES -o '{output_path}' -s {start_frame} -e {end_frame} -a\n",
        "else:\n",
        "  !sudo ./$blender_version/blender -P 'setgpu.py' -b '{path_to_blend}' -P 'setgpu.py' -o '{output_path}' -f {frame}\n"
      ],
      "execution_count": null,
      "outputs": []
    }
